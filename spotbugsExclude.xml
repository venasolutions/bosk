<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
		xmlns="https://github.com/spotbugs/filter/3.0.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

	<Match>
		<Bug code="EI"/>
	</Match>
	<Match>
		<Bug code="EI2"/>
	</Match>
	<Match>
		<Bug code="RCN"/>
	</Match>
	<Match>
		<Bug code="NP"/> <!-- https://github.com/spotbugs/spotbugs/issues/651 -->
	</Match>
	<Match>
		<Bug code="DP"/> <!-- doPrivileged is deprecated for removal -->
	</Match>
	<Match>
		<Class name="io.vena.bosk.Path"/>
		<Method name="empty"/>
		<Bug code="MS"/>
	</Match>
	<Match>
		<Class name="io.vena.bosk.Bosk$DefiniteReference"/>
		<Method name="dereferencer"/>
		<Bug code="JLM"/>
	</Match>
	<Match>
		<Class name="io.vena.bosk.drivers.mongo.SingleDocumentMongoDriver"/>
		<Method name="flush"/>
		<Bug pattern="SF_SWITCH_FALLTHROUGH"/>
	</Match>
</FindBugsFilter>
